{
  "name": "npm-version-lib",
  "version": "25.10.02-release.1",
  "description": "A TypeScript/JavaScript versioning library that generates build versions based on date and git commits",
  "bin": {
    "npm-version": "./dist/version-cli.js",
    "version-gen": "./dist/version-cli.js"
  },
  "homepage": "https://github.com/enVId-tech/NPM-VersionLib#readme",
  "bugs": {
    "url": "https://github.com/enVId-tech/NPM-VersionLib/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enVId-tech/NPM-VersionLib.git"
  },
  "license": "MIT",
  "author": "enVId-tech",
  "main": "./dist/generate-version.js",
  "types": "./dist/generate-version.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "dev:version:dev": "npx ts-node ./version-cli.ts dev",
    "dev:version:beta": "npx ts-node ./version-cli.ts beta",
    "dev:version:release": "npx ts-node ./version-cli.ts release",
    "dev:version": "npx ts-node ./version-cli.ts",
    "version:dev": "npx ts-node ./dist/version-cli.js dev",
    "version:beta": "npx ts-node ./dist/version-cli.js beta",
    "version:release": "npx ts-node ./dist/version-cli.js release",
    "version": "npx ts-node ./dist/version-cli.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@types/node": "^24.3.1"
  },
  "type": "module"
}